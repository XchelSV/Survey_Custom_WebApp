    style.
        .cropit-preview {
            background-color: #f8f8f8;
            background-size: cover;
            border: 1px solid #ccc;
            border-radius: 3px;
            margin-top: 7px;
            width: 400px;
            height: 91px;
          }

          /* Translucent background image */
        .cropit-preview-background {
          opacity: .2;
        }

        /*
         * If the slider or anything else is covered by the background image,
         * use relative or absolute position on it
         */
        input.cropit-image-zoom-input {
          position: relative;
        }

        /* Limit the background image by adding overflow: hidden */
        #image-cropper {
          overflow: hidden;
        }


    .container(ng-controller='addUserController')
        .row(style='margin-top:3%')
            .col.s8.offset-s2(align='center')
                h2.thin
                    | Registro de Usuario
        .row.animated.fadeIn
          form().col.s12
            .row
                .col.s6
                    .row
                      .input-field
                        input#first_name.validate(type='text',required, ng-model='nombre',ng-class='name_null')
                        label.regular(for='first_name') Nombre/Empresa
                    .row
                      .input-field
                        input#place.validate(type='text',required, ng-model='direccion',ng-class='dir_null')
                        label.regular(for='place') Direcci칩n
                    .row
                      .input-field
                        input#password.validate(type='password',required, ng-model='contrasena',ng-class='pass_null')
                        label.regular(for='password') Contrase침a
                    .row
                      .input-field
                        input#password.validate(type='password',required, ng-model='confirmar_contrasena')
                        label.regular(for='password') Confirmar Contrase침a
                    .row
                      .input-field
