                #image-cropper.col.s5.offset-s1(style='margin-top:4%')
                      // This is where the preview image is displayed
                      .row
                        .cropit-preview
                      
                      .row
                          // This range input controls zoom
                          // You can add additional elements here, e.g. the image icons
                          input.col.s6.cropit-image-zoom-input(type='range', ng-class='color')
                          .col.s6
                            button.waves-effect.waves-light.btn(ng-click='cut_img()',ng-class='color')
                              i.material-icons.right content_cut
                              | Cortar
                      .row
                          // This is where user selects new image
                          .input-field
                            .file-field.input-field
                              .btn(ng-class='color')
                                span Logo
                                input.cropit-image-input(type='file',ng-click='reenable()')
                              .file-path-wrapper
                                input.file-path.validate(type='text', required, ng-model='img',ng-class='img_null')

                      .row
                        .col.s12
                          a.waves-effect.waves-light.btn.modal-trigger(href='#modal1', ng-class='color') Paleta de Colores
                            i(class="material-icons right") color_lens


